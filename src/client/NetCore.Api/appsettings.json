{
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "ApplicationConnectionString": "#{ConnectionString.ApplicationConnectionString}",
    "IdpConnectionString": "#{ConnectionString.IdpConnectionString}",
    "MigrationsAssembly": "#{ConnectionString.MigrationsAssembly}",
    "Provider": "#{ConnectionString.Provider}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Information"
    },
    "Path": "C:\\Logs"
  },
  "AuthenticationServer": {
    "Audience": "#{AuthenticationServer.Audience}",
    "CertificatePassword": "#{AuthenticationServer.CertificatePassword}",
    "CertificatePath": "#{AuthenticationServer.CertificatePath}",
    "Issuer": "#{AuthenticationServer.Issuer}",
    "SecretKey": "#{AuthenticationServer.SecretKey}"
  }
}
