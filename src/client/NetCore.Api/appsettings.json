{
  "AllowedHosts": "*",
  "Database": {
    "ApplicationConnectionString": "#{Database.ApplicationConnectionString}",
    "IdpConnectionString": "#{Database.IdpConnectionString}",
    "MigrationsAssembly": "#{Database.MigrationsAssembly}",
    "Provider": "#{Database.Provider}",
    "RedisConnectionString": "#{Database.RedisConnectionString}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Information"
    },
    "Path": "D:\\Logs"
  },
  "AuthenticationServer": {
    "Audience": "#{AuthenticationServer.Audience}",
    "CertificatePassword": "#{AuthenticationServer.CertificatePassword}",
    "CertificatePath": "#{AuthenticationServer.CertificatePath}",
    "Issuer": "#{AuthenticationServer.Issuer}",
    "SecretKey": "#{AuthenticationServer.SecretKey}"
  }
}
